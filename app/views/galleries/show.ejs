<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.galleries %>">galleries</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of gallery</a></li>
</ul>

<div class="page-header">
    <h1>Details of gallery</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>name</th><td><%= gallery.name %></td></tr>

        <tr><th>description</th><td><%= gallery.description %></td></tr>

        <tr><th>createdAt</th><td><%= gallery.createdAt %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_gallery(gallery), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.gallery(gallery), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.galleries) %>
</div>
